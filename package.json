{
  "name": "elegant",
  "version": "2.0.0",
  "description": "GatsbyJS theme",
  "main": "none",
  "repository": "https://github.com/piecego/elegant.git",
  "author": "piecego <wktrf@foxmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "node node_modules/gatsby/dist/bin/gatsby build",
    "dev": "node node_modules/gatsby/dist/bin/gatsby develop",
    "serve": "node node_modules/gatsby/dist/bin/gatsby serve",
    "clean": "node node_modules/gatsby/dist/bin/gatsby clean",
    "develop-inspect": "node --inspect-brk --no-lazy node_modules/gatsby/dist/bin/gatsby develop",
    "gencode": "apollo client:codegen --target=typescript --localSchemaFile=schema.graphql --includes=src/**/*.tsx --tagName=graphql --addTypename --outputFlat src/types/gql.d.ts",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.+(js|ts|tsx|json|css|md)\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "dayjs": "^1.10.3",
    "immer": "^8.0.0",
    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "web-vitals": "^1.1.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@types/dotenv": "^8.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "apollo": "^2.32.1",
    "dotenv": "^8.2.0",
    "gatsby": "^2.30.1",
    "gatsby-cli": "^2.17.1",
    "gatsby-image": "^2.9.0",
    "gatsby-plugin-feed": "^2.11.0",
    "gatsby-plugin-google-analytics": "^2.9.0",
    "gatsby-plugin-less": "^4.5.0",
    "gatsby-plugin-manifest": "^2.10.0",
    "gatsby-plugin-react-helmet": "^3.8.0",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sharp": "^2.12.1",
    "gatsby-plugin-sitemap": "^2.10.0",
    "gatsby-plugin-svgr": "^2.1.0",
    "gatsby-plugin-typescript": "^2.10.0",
    "gatsby-remark-copy-linked-files": "^2.8.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.9.0",
    "gatsby-remark-katex": "^3.8.0",
    "gatsby-remark-prismjs": "^3.11.0",
    "gatsby-source-filesystem": "^2.9.0",
    "gatsby-transformer-remark": "^2.14.0",
    "gatsby-transformer-sharp": "^2.10.1",
    "graphql": "^15.5.0",
    "katex": "^0.12.0",
