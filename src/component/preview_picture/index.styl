.view_picture{
  position fixed
  width 100vw
  height 100vh
  z-index 999
  background-color rgba(255,255,255,.8)
  overflow hidden
  top 0
  left 0
  cursor zoom-out
  img{
    position absolute
    left 50%
    top 50%
    transform translate(-50%, -50%)
    object-fit cover
    max-width 90%
  }
}
